{
  "name": "optionstrat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "node scripts/db-migrate.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "phase2:dev": "ts-node src/phase2/server.ts",
    "phase2:build": "tsc --project tsconfig.phase2.json",
    "phase2:start": "node dist/phase2/server.js",
    "phase2:test": "jest --testPathPatterns=phase2",
    "phase2:test:watch": "jest --testPathPatterns=phase2 --watch",
    "phase25:test": "jest --testPathPatterns=phase25",
    "phase25:test:watch": "jest --testPathPatterns=phase25 --watch",
    "phase25:test:pbt": "jest --testPathPatterns=phase25 --testNamePattern='\\[PBT\\]'",
    "phase25:lint": "eslint src/phase25/**/*.ts",
    "phase25:typecheck": "tsc --noEmit --project tsconfig.json",
    "phase25:dev": "ts-node src/phase25/server.ts",
    "phase25:start": "node dist/phase25/server.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "next": "14.2.35",
    "pg": "^8.16.3",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "fast-check": "^4.5.3",
    "jest": "^30.2.0",
    "postcss": "^8",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
